{
  "git.autoRepositoryDetection": "subFolders",
  "git.detectSubmodules": false,
  "git.scanRepositories": [
    "."
  ],
  "git.ignoredRepositories": [
    "mindrift-Lionspark-weigh8",
    "weigh8-bulkconnections",
    "Mindrift-Orders",
    "weigh8-rebase-working",
    "weigh8-backend"
  ],
  "files.exclude": {
