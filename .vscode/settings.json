    "**/.next": true,
    "**/dist": true,
    "**/build": true,
    "postgres_data": true,
    "redis_data": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true,
    "**/package-lock.json": true,
    "postgres_data": true,
    "redis_data": true
  }
}
